.flex {
  display: flex;
  justify-content:center;
  flex-wrap: wrap;
}

.square {
  width: 200px;
  height: 200px;
  background-color: rgba(255,99,255,0.3);
  margin: 20px;
}

$circle-height = 200px;

#circle {
  width: 200px;
  height: $circle-height;
  border-radius: 9999rem;
  background-color: rgba(100,0,255, 0.3);
  margin: 20px;
  p {
    text-align: center;
    line-height: $circle-height;
    vertical-align: middle;
  }
}
